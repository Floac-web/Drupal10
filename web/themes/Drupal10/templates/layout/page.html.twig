
<div class="page">

  {{ page.header }}

  {{page.highlighted}}
  
  <main role="main">
    <div class="container">
    <div class="layout-content">
      {{ page.content }}
    </div>

    {% if page.sidebar %}
      <aside class="layout-sidebar" role="complementary">
        {{ page.sidebar}}
      </aside>
    {% endif %}
    </div>
  </main>

    <footer role="contentinfo">
      {{ page.footer }}
    </footer>


</div>
